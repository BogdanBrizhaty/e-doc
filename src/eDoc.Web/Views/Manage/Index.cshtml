@using eDoc.Web.Models
@model UserInfoWrapper
@{
    ViewBag.Title = "Мій аккаунт";
}
@Html.Partial("PersonalInfo", Model.UserInfo)
@if (Model.SpeciciedInfo is DoctorInfoModel)
{
    @Html.Partial("DoctorInfo", Model.SpeciciedInfo as DoctorInfoModel)
}
else
{
    @Html.Partial("PatientInfo", Model.SpeciciedInfo as PatientInfoModel)
}